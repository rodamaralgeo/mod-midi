[Unit]
Description=Mod-Midi Service, Start Jack Connection
After=jack2.service zynthian.service zynthian-webconf.service mod-ui.service

[Service]
Type=simple
ExecStart=/usr/bin/jack_connect "ZynMidiRouter:main_out" "mod-midi-preset:in_1"
Restart=always
RestartSec=2

[Install]
WantedBy=multi-user.target
